<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<type-mapping>
		<sql-type jdbc-type="INTEGER" hibernate-type="Integer"></sql-type>
		<sql-type jdbc-type="VARCHAR" hibernate-type="String"></sql-type>
		<sql-type jdbc-type="BOOLEAN" hibernate-type="Boolean"></sql-type>
	</type-mapping>
	<table-filter match-name="secaf" exclude="true"
		match-schema="catalogo" />
	<table-filter match-schema="inventario" match-name=".*"
		exclude="true" />
	<table-filter match-name="marcas_modelos" match-schema="catalogo"
		exclude="true" />
	<table-filter match-name="caracteristicas_hardware" exclude="true"
		match-schema="catalogo">
	</table-filter>
	<table-filter match-name="tipo_activo_caracteristicas_hardware"
		exclude="true" match-schema="catalogo" />
	<table-filter match-name="componentes_activos"
		match-schema="soportetecnico" exclude="true">
	</table-filter>
</hibernate-reverse-engineering>